{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "version": "1.0.1",
    "name": "fims"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "favicon.png",
        "assets"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "styles.scss",
        "theme.scss",
        "../node_modules/@covalent/core/common/platform.css",
        "./assets/css/vendor/bootstrap.min.css",
        "./assets/css/vendor/bootstrap.rtl.only.min.css",
        "../node_modules/@glidejs/glide/dist/css/glide.core.min.css",
        "../node_modules/quill/dist/quill.snow.css",
        "../node_modules/quill/dist/quill.bubble.css",
        "../node_modules/@ng-select/ng-select/themes/default.theme.css",
        "../node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
        "../node_modules/nouislider/distribute/nouislider.min.css",
        "../node_modules/angular-archwizard/archwizard.css",
        "../node_modules/ngx-lightbox/lightbox.css",
        "../node_modules/video.js/dist/video-js.min.css",
        {
          "input": "./assets/css/sass/themes/vien.light.blueolympic.scss",
          "bundleName": "light.blue.olympic",
          "inject": false
        }
      ],
      "scripts": [
        "../node_modules/hammerjs/hammer.min.js",
        "../node_modules/showdown/dist/showdown.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "serve": {
      "port": 4200,
      "host": "localhost"
    }
  }
}
